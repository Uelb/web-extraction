{
  "manifest_version": 2,

  "name": "SSQL web data extraction",
  "description": "This extension helps you provide web sources of data for SuperSQL",
  "version": "0.1",
  "icons": { "128": "img/icon.png" },

  "permissions": [
     "activeTab"
  ],
  "background": {
    "scripts": ["src/main.js"],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "img/icon.png"
  }
}